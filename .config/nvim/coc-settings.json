{
  "coc.preferences.formatOnSaveFiletypes": [
    "elm",
    "json",
    "javascript",
    "typescript",
    "typescriptreact",
    "css"
  ],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "codeLens.enable": true,
  "prettier.formatterPriority": 999
}
